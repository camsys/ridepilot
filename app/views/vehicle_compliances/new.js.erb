var $new_form = $("<%= j (render partial: 'form', locals: {remote: true}) %>");
$new_form.find('input.datepicker').datepicker({dateFormat: 'yy-mm-dd', showButtonPanel: true}).css('z-index', 9999);

$('#modal-dialog .modal-body .container-fluid').html($new_form);
$('#modal-dialog .modal-title').html("<%= j translate_helper('vehicle_compliance_form_new_heading') %>");
$('#modal-dialog').modal('show');
